 					file->f_path.dentry->d_parent->d_inode,
 					file, bstart, bindex, size);
-				if (likely(!err))
+				if (!err)
 					break;
 			}
 			return err;
@@ -499,7 +499,7 @@ static int __open_file(struct inode *inode, struct file *file)
 		dentry_open(lower_dentry,
 			    unionfs_lower_mnt_idx(file->f_path.dentry, bstart),
 			    lower_flags);
